{
	"geode": "4.8.0",
	"gd": {
		"win": "2.2074",
		"mac": "2.2074",
		"android": "2.2074",
		"ios": "2.2074"
	},
	"id": "astral.bot",
	"name": "Astral",
	"version": "1.0.0-alpha.4",
	"developers": ["Astral Team", "GWDdoS", "Argxment", "LoliGD", "slideglide"],
	"description": "Astral: A multi-platform bot made for botting hard GD levels such as the ILL and the SLL.",
	"resources": {
		"files": [
			"resources/font0.ttf"
		]
	},
	"links": {
		"source": "https://github.com/GWDdoS/Astral",
		"community": "https://discord.gg/XbBVg2mCUD"
	},
	"tags": ["gameplay", "utility", "cheat"],
	"settings": {
		"ffmpeg-path": {
			"type": "file",
			"name": "FFmpeg Path",
			"description": "The path of the FFmpeg executable (ffmpeg.exe), required for renderer",
			"platforms": ["win"],
			"default": {
				"win": "{gd_dir}/.astral/ffmpeg/ffmpeg.exe"
			},
			"control": {
				"dialog": "open",
				"filters": [
					{
						"files": ["*.exe"],
						"description": "ffmpeg.exe"
					}
				]
			}
		}
	},
	"dependencies": {
        "geode.node-ids": {
            "version": ">=v1.21.1",
            "importance": "required"
        }
	}
}
